<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".nyudhamacharya.musthi_pratyakaraman.Musthi_pratyakaraman_4">


    <LinearLayout android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@color/colorPrimaryDark"
        android:layout_marginTop="10.0dip">
        <RelativeLayout android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20.0dip"
            android:layout_marginTop="0dip"
            android:layout_marginRight="20.0dip">
            <TextView
                android:textSize="22.0sp"
                android:textColor="#ffff"
                android:textStyle="bold"
                android:gravity="center"

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="व्यायाम शिक्षक श्रेणी मुष्टि प्रहार - सुरक्षा प्रत्याक्रमण " />
            <TextView
                android:textSize="20.0sp"

                android:textColor="#ffff"
                android:textStyle="bold"
                android:gravity="center"

                android:layout_marginTop="50dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="परशु स्कन्ध विश्राम सज्ज" />
        </RelativeLayout>
        <androidx.cardview.widget.CardView
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/card1"
            android:layout_width="fill_parent"
            android:layout_height="140.0dip">
            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent">

                <TextView android:textSize="17.0sp"
                    android:textStyle="bold"
                    android:textColor="@color/cardview_dark_background"
                    android:layout_gravity="start"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15.0dip"
                    android:text="1.अ दाहिनी मुष्टि का प्रहार मुख पर करे । ब अपर आउट साइड ब्लाक बांये हाथ से करके उसकी कलाई को पकड़ ले । अ पुनःठ बांया मुष्टि प्रहार मुख पर करे । ब दाहिने हाथ को श्येन प्रहार (ईगल पंच) बनाकर उसे दाहिनी ओर धकेल कर उसकी कलाई को पकड़ ले । ब अपने दाहिने पैर की किक अ के अण्डकोष और फिर छाती में मारे ।"
                    android:layout_centerVertical="true"
                    android:fontFamily="serif" />

            </RelativeLayout>
        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/card2"
            android:layout_width="fill_parent"
            android:layout_height="120.0dip"
            android:layout_marginTop="15.0dip">
            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent">


                <TextView android:textSize="15.0sp"
                    android:textStyle="bold"
                    android:textColor="@color/cardview_dark_background"
                    android:layout_gravity="start"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15.0dip"
                    android:text="2.अ दाहिना मुष्टि प्रहार करे । ब दाहिने हाथ को मोड़कर दाहिने से बांये प्रहार को धक्का देकर बचाये और बांये हाथ से अ की कलाई पकड़ ले । ब दाहिने हाथ से अ की ओर पेट का मांस और चर्म पकड़ कर दाहिने पैर बांयीकी टंगी अ के दाहिने पैर के पीछे मारे और दाहिनी कोहनी का प्रहार अ की बांयी कनपटी पर मारते हुये उसे बांयी ओर भूमि पर गिरा दे। "
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:fontFamily="serif" />
            </RelativeLayout>
        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/card3"
            android:layout_width="fill_parent"
            android:layout_height="90.0dip"
            android:layout_marginTop="15.0dip">
            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent">

                <TextView android:textSize="15.0sp"
                    android:textStyle="bold"
                    android:textColor="@color/cardview_dark_background"
                    android:layout_gravity="start"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15.0dip"
                    android:text="3.अ सामने मुष्टि प्रहार करे। व अश्वारोही स्थिति में खड़ा हुआ सामने आये प्रहार को दाहिने हाथ की हथेली से बांयी ओर धक्का मार कर बचाये तथा उसी हाथ की उलटी मुट्ठी का प्रहार अ की दाहिनी कनपटी पर मारे। "
                    android:layout_centerVertical="true"
                    android:fontFamily="serif" />
            </RelativeLayout>
        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/card4"
            android:layout_width="fill_parent"
            android:layout_height="190.0dip"
            android:layout_marginTop="15.0dip">
            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent">


                <TextView android:textSize="15.0sp"
                    android:textStyle="bold"
                    android:textColor="@color/cardview_dark_background"
                    android:layout_gravity="start"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15.0dip"
                    android:text="4.अ दाहिना मुष्टि प्रहार मुख पर करे। ब घूमकर दाहिने हाथ से उसे रोके और अ का पहुंचा पकड़ ले। अ बांयी ओर से पीछे घूमकर बांयी मुट्ठी का उलटा प्रहार ब की बांयी कनपटी पर मारे । ब बांये हाथ का आउट ब्लाक करके प्रहार को रोके और उसका पहुंचा पकड़ ले।अ दोनों हाथों के पकड़े जाने पर बांये पैर की बैक किक ब के कलेजे पर मारकर हाथों को छुड़ा ले।
 सुरक्षा – पहले किये मुष्टि प्रहार को दाहिने हाथ का इन साइड ब्लाक (कोहनी मुड़ी हुई) से करे। दूसरे प्रहार की सुरक्षा बांया पैर पीछे क्रास करके बायें हाथ से करे तथा पीछे आई बैक किक को घुटने से पीछे दाहिने हाथ से दबा दे तथा स्वयं घड़ी की विपरीत दिशा में कुछ घूम कुछ बांयी ओर जाये। "
                    android:layout_centerVertical="true"
                    android:fontFamily="serif" />
            </RelativeLayout>
        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/card5"
            android:layout_width="fill_parent"
            android:layout_height="130.0dip"
            android:layout_marginTop="15.0dip">
            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent">

                <TextView android:textSize="15.0sp"
                    android:textStyle="bold"
                    android:textColor="@color/cardview_dark_background"
                    android:layout_gravity="start"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15.0dip"
                    android:text="5. एक हाथ से आक्रमण एवं सुरक्षा । अ दाहिना पैर आगे
बढ़ाकर सीधी अंगुलियों से भल्ल प्रहार ब के माथे पर मारे और साथ ही उसी हाथ की हथेली से कलेजे पर प्रहार करे । ब दाहिना मुष्टि प्रहार नाक पर करे। अ दाहिने हाथ के किनारे से आउट ब्लाक करे। ब बांये हाथ से मुष्टि प्रहार करे। अ दाहिने हाथ के किनारे से आउट ब्लाक करे।"
                    android:layout_centerVertical="true"
                    android:fontFamily="serif" />
            </RelativeLayout>
        </androidx.cardview.widget.CardView>


        <androidx.cardview.widget.CardView
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/card6"
            android:layout_width="fill_parent"
            android:layout_height="150.0dip"
            android:layout_marginTop="15.0dip">
            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent">

                <TextView android:textSize="15.0sp"
                    android:textStyle="bold"
                    android:textColor="@color/cardview_dark_background"
                    android:layout_gravity="start"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15.0dip"
                    android:text="6.अ सामने मुष्टि प्रहार करे। ब दाहिना पैर कुछ दाहिनी ओर रख कर थोड़ा घड़ी की विपरीत दिशा में घूमकर प्रहार को बचाये तथा बांये हाथ से उसका पहुंचा पकड़ ले एवं दाहिने हाथ को उलट कर अ के पेट की मांसपेशियों तथा चर्म को पकड़ ले और दाहिने पैर की साइड किक अ के कलेजे पर मारे। "
                    android:layout_centerVertical="true"
                    android:fontFamily="serif" />
            </RelativeLayout>
        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/card7"
            android:layout_width="fill_parent"
            android:layout_height="130.0dip"
            android:layout_marginTop="15.0dip">
            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent">

                <TextView android:textSize="15.0sp"
                    android:textStyle="bold"
                    android:textColor="@color/cardview_dark_background"
                    android:layout_gravity="start"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15.0dip"
                    android:text="7.अ, ब को दोनों हाथों से पकड़ने का प्रयत्न करे । ब दाहिना
पैर आगे बढ़ाकर दोनों हाथों को बाहर की ओर करते हुये प्रहार की दिशा बदल दे तथा कुछ नीचे झुककर दोनों हाथों अ के पेट को पार्श्व भागों में पकड़ कर अपनी ओर अनेक सेबार खींचे एवं उसके कलेजे पर अपने सिर का अनेक बार प्रहार करके उसके श्वास को बन्ध कर दे।अ सामने मुष्टि प्रहार करे । ब दाहिनी ओर घूमकर प्रहार को खाली करे और बांये हाथ से अ का पहुंचा पकड़ ले तथा दाहिने हाथ को उलट कर अ की दाहिनी ओर ग्रीवा की पेशियों को पकड़ कर खींचे एवं इसी स्थिति में दाहिने पैर की स्पिन किक बांयी ओर उसके मुख पर मारे।"
                    android:layout_centerVertical="true"
                    android:fontFamily="serif" />
            </RelativeLayout>
        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/card8"
            android:layout_width="fill_parent"
            android:layout_height="130.0dip"
            android:layout_marginTop="15.0dip">
            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent">

                <TextView android:textSize="15.0sp"
                    android:textStyle="bold"
                    android:textColor="@color/cardview_dark_background"
                    android:layout_gravity="start"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15.0dip"
                    android:text=" 8.अ सामने मुष्टि प्रहार करे । ब दाहिने हाथ को मोड़कर उससे बांयी ओर धक्का मार कर बचाव करे तथा बांये हाथ से अ का पहुंचा पकड़ ले। अ बांये हाथ का मुष्टि प्रहार करे। ब दाहिने हाथ को बदलकर उससे सुरक्षा करे और अ का बांया पहुंचा पकड़ ले । ब दाहिने पैर की किक टखने से ऊपर वाले भाग से अ की बांयी ओर पसलियों पर मारे। इसी भांति बांये पैर की किक दाहिनी ओर
पसलियों पर मारे। "
                    android:layout_centerVertical="true"
                    android:fontFamily="serif" />
            </RelativeLayout>
        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/card9"
            android:layout_width="fill_parent"
            android:layout_marginBottom="15dp"
            android:layout_height="130.0dip"
            android:layout_marginTop="15.0dip">
            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent">

                <TextView android:textSize="15.0sp"
                    android:textStyle="bold"
                    android:textColor="@color/cardview_dark_background"
                    android:layout_gravity="start"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15.0dip"
                    android:text="9.अ दाहिना पैर आगे बढ़ाकर बांये हाथ का रिवर्स पंच ब के
मुख पर मारे । ब दाहिने पैर को आगे बढ़ाकर दाहिने हाथ के आउट साइड ब्लाक से उसे रोके और दाहिनी चाप अ की बांयी कनपटी पर मारे। अ दाहिने हाथ की कलाई से उसे दाहिने से बायें काट दे तथा स्वयं दाहिने हाथ की उलटी मुट्ठी का प्रहार ब की दाहिनी कनपटी पर मारे । ब बांये हाथ की अर्ध चन्द्र चाप द्वारा उसे बांये से दाहिने ओर काट दे तथा अ कीक लाई पकड़ ले।
बन्ध लगाना, ब अ के गले पर दाहिने हाथ का श्येन प्रहार"
                    android:layout_centerVertical="true"
                    android:fontFamily="serif" />
            </RelativeLayout>
        </androidx.cardview.widget.CardView>


    </LinearLayout>

</ScrollView>